    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "document-title": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": "off",
    "disown-opener": "off",
    "axe/language": [
